#!/usr/bin/env sh
# Gradle start-up script for UN*X

DIRNAME=$(dirname "$0")
GRADLE_HOME=
APP_BASE_NAME=$(basename "$0")

# Locate Java
if [ -z "$JAVA_HOME" ] ; then
  JAVA_EXE=java
else
  JAVA_EXE="$JAVA_HOME/bin/java"
fi

exec "$JAVA_EXE" -classpath "$DIRNAME/gradle/wrapper/gradle-wrapper.jar" org.gradle.wrapper.GradleWrapperMain "$@"
